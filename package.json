{
  "name": "file-tree",
  "version": "1.0.0",
  "description": "A command-line tool to display directory tree structure",
  "main": "dist/tree-cli.js",
  "bin": {
    "tree-cli": "./dist/tree-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [
    "cli",
    "tree",
    "directory",
    "file-system"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "commander": "^11.0.0"
  }
}
